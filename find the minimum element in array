#include <stdio.h>

int main() {
    int arr[] = {10, 5, 25, 3, 18};
    int n = sizeof(arr) / sizeof(arr[0]);
    int min = arr[0];   // Assume first element is minimum

    for(int i = 1; i < n; i++) {
        if(arr[i] < min) {
            min = arr[i];
        }
    }

    printf("Minimum element = %d", min);

    return 0;
}
