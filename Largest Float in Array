#include <stdio.h>

int main() {
    int n;

   
    if (scanf("%d", &n) != 1) {
        return 0;
    }

    float arr[n];

   
    for (int i = 0; i < n; i++) {
        scanf("%f", &arr[i]);
    }

    float max = arr[0];

    
    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }

  
    printf("%.2f\n", max);

    return 0;
}
