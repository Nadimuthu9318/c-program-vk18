#include <stdio.h>

int main() {
    int N;
    
    if (scanf("%d", &N) != 1) return 0;

   
    long long expectedSum = (long long)N * (N + 1) / 2;
    long long actualSum = 0;
    int rollNumber;

   
    for (int i = 0; i < N - 1; i++) {
        scanf("%d", &rollNumber);
        actualSum += rollNumber;
    }

    int missingNumber = (int)(expectedSum - actualSum);

    printf("%d\n", missingNumber);

    return 0;
}
